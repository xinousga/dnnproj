CREATE PROCEDURE {databaseOwner}{objectQualifier}M2Land_FlashImageRotatorUpdate
	@ModuleID int, 
	@Source nvarchar(100),
	@Height int, 
	@Width int, 
	@File nvarchar(100), 
	@Image nvarchar(100), 
	@ID nvarchar(100), 
	@SearchBar bit, 
	@BackColor char(8), 
	@FrontColor char(8), 
	@LightColor char(8), 
	@ScreenColor char(8), 
	@Logo nvarchar(100), 
	@OverStrech char(10),
	@ShowEq bit, 
	@ShowIcons bit, 
	@Transtion char(10), 
	@ShowNavigation bit, 
	@ShowStop bit, 
	@ShowDigits bit, 
	@ShowDownload bit, 
	@UseFullScreen bit, 
	@AutoScroll bit, 
	@DisplayHeight int, 
	@DisplayWidth int, 
	@ThumbsInPlaylist bit, 
	@Audio nvarchar(100), 
	@AutoStart bit, 
	@BufferLength int, 
	@Captions char(10), 
	@FallBack nvarchar(100), 
	@Repeat char(10),
	@RotateTime int, 
	@Shuffle bit, 
	@Smoothing bit, 
	@Volume int, 
	@CallBack varchar(10), 
	@EnableJS bit, 
	@JavascriptID varchar(50), 
	@Link nvarchar(100), 
	@LinkFromDisplay bit, 
	@LinkTarget char(10), 
	@Recommendation nvarchar(100), 
	@SearchLink nvarchar(100), 
	@StreamScript nvarchar(100), 
	@Type char(10) 
AS

UPDATE {objectQualifier}M2Land_FlashImageRotator SET
	[ModuleID] = @ModuleID,
	[Source] = @Source,
	[Height] = @Height,
	[Width] = @Width,
	[File] = @File,
	[Image] = @Image,
	[ID] = @ID,
	[SearchBar] = @SearchBar,
	[BackColor] = @BackColor,
	[FrontColor] = @FrontColor,
	[LightColor] = @LightColor,
	[ScreenColor] = @ScreenColor,
	[Logo] = @Logo,
	[OverStrech] = @OverStrech,
	[ShowEq] = @ShowEq,
	[ShowIcons] = @ShowIcons,
	[Transtion] = @Transtion,
	[ShowNavigation] = @ShowNavigation,
	[ShowStop] = @ShowStop,
	[ShowDigits] = @ShowDigits,
	[ShowDownload] = @ShowDownload,
	[UseFullScreen] = @UseFullScreen,
	[AutoScroll] = @AutoScroll,
	[DisplayHeight] = @DisplayHeight,
	[DisplayWidth] = @DisplayWidth,
	[ThumbsInPlaylist] = @ThumbsInPlaylist,
	[Audio] = @Audio,
	[AutoStart] = @AutoStart,
	[BufferLength] = @BufferLength,
	[Captions] = @Captions,
	[FallBack] = @FallBack,
	[Repeat] = @Repeat,
	[RotateTime] = @RotateTime,
	[Shuffle] = @Shuffle,
	[Smoothing] = @Smoothing,
	[Volume] = @Volume,
	[CallBack] = @CallBack,
	[EnableJS] = @EnableJS,
	[JavascriptID] = @JavascriptID,
	[Link] = @Link,
	[LinkFromDisplay] = @LinkFromDisplay,
	[LinkTarget] = @LinkTarget,
	[Recommendation] = @Recommendation,
	[SearchLink] = @SearchLink,
	[StreamScript] = @StreamScript,
	[Type] = @Type
