AS

SELECT
	[ModuleID],
	[Source],
	[Height],
	[Width],
	[File],
	[Image],
	[ID],
	[SearchBar],
	[BackColor],
	[FrontColor],
	[LightColor],
	[ScreenColor],
	[Logo],
	[OverStrech],
	[ShowEq],
	[ShowIcons],
	[Transtion],
	[ShowNavigation],
	[ShowStop],
	[ShowDigits],
	[ShowDownload],
	[UseFullScreen],
	[AutoScroll],
	[DisplayHeight],
	[DisplayWidth],
	[ThumbsInPlaylist],
	[Audio],
	[AutoStart],
	[BufferLength],
	[Captions],
	[FallBack],
	[Repeat],
	[RotateTime],
	[Shuffle],
	[Smoothing],
	[Volume],
	[CallBack],
	[EnableJS],
	[JavascriptID],
	[Link],
	[LinkFromDisplay],
	[LinkTarget],
	[Recommendation],
	[SearchLink],
	[StreamScript],
	[Type]
FROM {objectQualifier}M2Land_FlashImageRotator
WHERE
	[ModuleID]=@ModuleID 
GO

